<%- content_for :head do-%>
<title>管理画面&nbsp;|&nbsp;ファイル&nbsp;|&nbsp;編集</title>
<%- end -%>
<div class="row">
  <div class="col s12 m10 offset-m1 l10 offset-l1">
    <div class="card">
      <%= form_for(@material, url: material_path, method: :patch) do |f| %>
        <div class="card-content">
          <span class="card-title black-text">ファイル&nbsp;編集</span>
          <%= render "registration", f: f %>
        </div>
        <div class="card-action center-align">
          <%= f.button '更新する', class: 'waves-effect waves-light btn white-text wine-red', data: { confirm: '更新してもよろしいですか?' } %>
        </div>
      <% end %>
    </div>
  </div>
</div>
